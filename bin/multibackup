#!/usr/bin/env python

import os
import yaml

from multibackup.command import parser

if __name__ == '__main__':
    if os.geteuid() == 0:
        raise OSError('Do not run as root!')
    args = parser.parse_args()
    with open(args.config_file) as fh:
        config = yaml.safe_load(fh)
    args.func(config, args)
